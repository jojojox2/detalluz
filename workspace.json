{
  "version": 1,
  "projects": {
    "api": {
      "root": "libs/api",
      "sourceRoot": "libs/api/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/api/**/*.ts"]
          }
        }
      },
      "tags": []
    },
    "detalluz-app": {
      "projectType": "application",
      "root": "apps/detalluz-app",
      "sourceRoot": "apps/detalluz-app/src",
      "prefix": "(dtl|detalluz)",
      "i18n": {
        "sourceLocale": "en",
        "locales": {
          "es": "apps/detalluz-app/src/locales/messages.es.xlf"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "localize": false,
            "i18nMissingTranslation": "warning",
            "outputPath": "dist/apps/detalluz-app",
            "index": "apps/detalluz-app/src/index.html",
            "main": "apps/detalluz-app/src/main.ts",
            "polyfills": "apps/detalluz-app/src/polyfills.ts",
            "tsConfig": "apps/detalluz-app/tsconfig.app.json",
            "inlineStyleLanguage": "sass",
            "assets": [
              "apps/detalluz-app/src/favicon.ico",
              "apps/detalluz-app/src/assets"
            ],
            "styles": ["apps/detalluz-app/src/styles.sass"],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/material/src/lib"]
            }
          },
          "configurations": {
            "production": {
              "localize": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/detalluz-app/src/environments/environment.ts",
                  "with": "apps/detalluz-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "es": {
              "localize": ["es"],
              "baseHref": "/es"
            },
            "en": {
              "localize": ["en"],
              "baseHref": "/en"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "detalluz-app:build:production"
            },
            "development": {
              "browserTarget": "detalluz-app:build:development",
              "proxyConfig": "apps/detalluz-app/proxy.conf.json"
            },
            "es": {
              "browserTarget": "detalluz-app:build:development,es",
              "proxyConfig": "apps/detalluz-app/proxy.conf.json"
            },
            "en": {
              "browserTarget": "detalluz-app:build:development,en",
              "proxyConfig": "apps/detalluz-app/proxy.conf.json"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "detalluz-app:build",
            "outputPath": "apps/detalluz-app/src/locales"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/detalluz-app/src/**/*.ts",
              "apps/detalluz-app/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/detalluz-app"],
          "options": {
            "jestConfig": "apps/detalluz-app/jest.config.ts",
            "codeCoverage": true,
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "detalluz-app-e2e": {
      "root": "apps/detalluz-app-e2e",
      "sourceRoot": "apps/detalluz-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/detalluz-app-e2e/cypress.json",
            "devServerTarget": "detalluz-app:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "detalluz-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/detalluz-app-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["detalluz-app"]
    },
    "detalluz-lambda": {
      "root": "apps/detalluz-lambda",
      "sourceRoot": "apps/detalluz-lambda/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/workspace:run-script",
          "options": {
            "script": "netlify-lambda build apps/detalluz-lambda/src"
          }
        },
        "serve": {
          "builder": "@nrwl/workspace:run-script",
          "options": {
            "script": "netlify-lambda serve apps/detalluz-lambda/src"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/detalluz-lambda/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/detalluz-lambda"],
          "options": {
            "jestConfig": "apps/detalluz-lambda/jest.config.ts",
            "codeCoverage": true,
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "material": {
      "projectType": "library",
      "root": "libs/material",
      "sourceRoot": "libs/material/src",
      "prefix": "(dtl|detalluz)",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/material"],
          "options": {
            "jestConfig": "libs/material/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/material/src/**/*.ts",
              "libs/material/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "services": {
      "projectType": "library",
      "root": "libs/services",
      "sourceRoot": "libs/services/src",
      "prefix": "(dtl|detalluz)",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/services"],
          "options": {
            "jestConfig": "libs/services/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/services/src/**/*.ts",
              "libs/services/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "shared": {
      "projectType": "library",
      "root": "libs/shared",
      "sourceRoot": "libs/shared/src",
      "prefix": "(dtl|detalluz)",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared"],
          "options": {
            "jestConfig": "libs/shared/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/src/**/*.ts",
              "libs/shared/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "ui": {
      "projectType": "library",
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "prefix": "(dtl|detalluz)",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui"],
          "options": {
            "jestConfig": "libs/ui/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui/src/**/*.ts", "libs/ui/src/**/*.html"]
          }
        }
      },
      "tags": []
    }
  }
}
